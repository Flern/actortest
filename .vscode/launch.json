    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/bin/Debug/net6.0/actortest.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET with Dapr",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/src/bin/Debug/net6.0/actortest.dll",
            "preLaunchTask": "daprd-debug-dotnet",
            "postDebugTask": "daprd-down-dotnet"
        }
    ]
}